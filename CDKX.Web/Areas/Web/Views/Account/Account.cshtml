</head>
<body>
    <!-- win-bar -->
    @RenderPage("~/Areas/Web/Views/Shared/TopHtml.cshtml")
    <!-- //header -->

    <div class="personal cl con pos-r">
        <div class="personalBar l">
            <div class="personal-btn pl-20">
                <div class="personal-btn-one styleall mb-20">
                    <p class='f-14 p-colorH'>个人中心</p>
                </div>
                <div class="personal-btn-some style1 mb-20">
                    <ul class="pl-10 f-12 lh-22">
                        <li><a href="javascript:void(0);" class="p-colorH list-active">我的作品</a></li>
                        <li><a href="javascript:void(0);" class="p-colorH">订单</a></li>
                        <li><a href="javascript:void(0);" class="p-colorH">我的售后</a></li>
                        <li><a href="javascript:void(0);" class="p-colorH">个人信息</a></li>
                        <li><a href="javascript:void(0);" class="p-colorH">账户安全</a></li>
                        <li><a href="javascript:void(0);" class="p-colorH">收货地址</a></li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="personalCon  l">
            <div class="personalCon-bar mb-50">
                <p class="f-16 mb-20">
                    我的作品(<span>@positions.Count()</span>)
                </p>
                <ul class="mywork">
                    @{
                        foreach (Bode.Services.Core.Models.CustomUser.Position position in positions)
                        {
                            List<Bode.Services.Core.Models.CustomMade.Material> materials = ViewBag.Mater;

                    <li class="goods-car cl pd-20 pos-r aaaaa">
                        <div class="close-btn" data-id="@position.Id" onclick="deletePosition(this)"></div>
                        <div class="check-box check-item" data-id="@position.Id">
                            <input type="checkbox" name="checkbox" id="checkbox" class="checkBox l round ">
                        </div>
                        <div class="goods-pic l  text-c mr-20">
                            <img src="@position.Cover">
                        </div>
                        <div class="goods-fun l col-4-3 cl">
                            <div class="goods-fun-title f-18 pt-20 pb-20">
                                @position.Name
                                <p class="text-r p-colorR r">￥<span class="price">@position.Price</span></p>
                            </div>
                            <div class="goods-fun-tab pt-20 pl-20">
                                <ul class="cl p-colorH lh-24 goodsData">
                                    @*<li class="l col-3-1 ">跟高：<span class="HeelHight">高跟(5-8cm)</span></li>*@
                                    <li class="l col-3-1">配饰：<span class="gAcc">@materials.Single(x => x.Id == position.AccMaterial).Name</span> </li>
                                    @*<li class="l col-3-1"> 内里材质：<span class="gInMaterial">人造革/PU</span></li>*@
                                    @{
                            if (position.FaceMaterial != null)
                            {
                                            <li class="l col-3-1">鞋面材质：<span class="gMaterial">@materials.First(x => x.Id == position.FaceMaterial.MaterialId).Name</span></li>
                                            <li class="l col-3-1">鞋底材质：<span class="gsoleMaterial">@materials.First(x => x.Id == position.HeelMaterial.MaterialId).Name</span></li>
                            }

                            {
                                            <li class="l col-3-1">鞋面材质：<span class="gMaterial">无</span></li>
                                            <li class="l col-3-1">鞋底材质：<span class="gsoleMaterial">无</span></li>
                            }
                                    }


                                </ul>
                            </div>
                            <div class="addgoodCar f-16 text-r r" onclick="positionToGoods('@position.Id')">加入购物车</div>
                        </div>
                    </li>
                        }
                    }
                    @{if (positions.Count() > 0)
                    {
                    <div class="f-16 p-colorH p-pointer">
                        <div class="check-box p-dlb checkall">
                            <input type="checkbox" name="checkbox" id="checkbox2" class="checkBox l round" onclick="selecetAll(this)">
                            <label for='checkbox2' class="f-16">
                                全选
                            </label>
                        </div>
                        <div class="delet p-dlb" id="delete" onclick="deleteAll()">删除选中的产品</div>
                    </div>
                    }}
                </ul>
            </div>

            <div class="personalCon-bar" id="tab-order">
                <p class="f-16 mb-20">订单</p>
                <ul class="cl mb-40 tabBar" id="orderUl">
                    <li class="l mr-50 current">
                        <a href="javascript:void(0);" class="l mr-50 p-colorH f-14 t-list disAll" data-id="0">全部订单</a>
                    </li>
                    <li class="l mr-50">
                        <a href="javascript:void(0);" class="p-colorH f-14 t-list"  data-id="1">待付款</a>
                    </li>
                    <li class="l mr-50">
                        <a href="javascript:void(0);" class="p-colorH f-14 t-list"  data-id="2">待生产</a>
                    </li>
                    <li class="l mr-50">
                        <a href="javascript:void(0);" class="p-colorH f-14 t-list"  data-id="3">生产中</a>
                    </li>
                    <li class="l mr-50">
                        <a href="javascript:void(0);" class="p-colorH f-14 t-list"  data-id="4">待收货</a>
                    </li>
                    <li class="l mr-50">
                        <a href="javascript:void(0);" class="p-colorH f-14 t-list"  data-id="5">已完成</a>
                    </li>
                    <li class="l mr-50">
                        <a href="javascript:void(0);" class="p-colorH f-14 t-list"  data-id="6">已取消</a>
                    </li>
                </ul>
                <div class="bar" id="bar0">

                </div>
                <div class="bar" id="bar1">

                </div>
                <div class="bar" id="bar2">

                </div>
                <div class="bar" id="bar3">

                </div>
                <div class="bar" id="bar4">

                </div>
                <div class="bar" id="bar5">

                </div>
                <div class="bar" id="bar6">

                </div>
            </div>
            <!-- 我的售后-->
            <div class="personalCon-bar" id="tab-custom">
                <p class="f-16 mb-20">我的售后</p>
            <ul class="cl mb-40 tabBar" id="shouhouUl">
                <li class="l mr-50">
                    <a class="l mr-50 f-14 p-colorH disAll" data-id="0">全部售后</a>
                </li>
                <li class="l mr-50">
                    <a href="javascript:void(0);" class="p-colorH f-14" data-id="1">审核中</a>
                </li>
                <li class="l mr-50">
                    <a href="javascript:void(0);" class="p-colorH f-14" data-id="2">退货中</a>
                </li>
                <li class="l mr-50">
                    <a href="javascript:void(0);" class="p-colorH f-14" data-id="3">退货失败</a>
                </li>
                <li class="l mr-50">
                    <a href="javascript:void(0);" class="p-colorH f-14" data-id="4">退货成功</a>
                </li>
            </ul>
            <div class="bar" id="s_bar0">
            </div>
                    <!-- 审核中 -->
                    <div class="bar" id="s_bar1">
                    </div>
                    <!--退货中-->
                    <div class="bar goods" id="s_bar2">
                    </div>
                    <!--退货失败-->
                <div class="bar" id="s_bar3">
                </div>
                    <!--退货成功-->
                <div class="bar" id="s_bar4">
                </div>
                </div>
            <!-- 个人信息-->
            <div class="personalCon-bar">
                <p class="f-16 mb-40">个人信息</p>
                <form id="personalVaild" action="" method="post">
                    <ul class="personal-info">
                        <li class="f-14 pb-20 mb-20 p-colorB bt">
                            <div>
                                <span class="p-dlb w-80">手机号</span><span class="dot">:</span><span class="result" id="phoneNo3">@userInfo.SysUser.UserName</span>
                            </div>
                        </li>
                        <li class="f-14 pb-20 mb-20 p-colorB bt">
                            <div class="viewShow">
                                <span class="p-dlb w-80">昵称</span><span class="dot">:</span><span class="result">@userInfo.SysUser.NickName</span>
                                <div class="f-r"><a href="javascript:void(0);" class="p-dlb editor mt-5 mr-10 editN"></a><span class="">编辑</span></div>
                            </div>
                            <div class="viewShow hide">
                                <span class="p-dlb w-80">昵称</span>
                                <div class="skin-minimal p-dlb">
                                    <input type="text" placeholder="" class="input-text size-M" id="NickName" name="username" value="@userInfo.SysUser.NickName">
                                </div>
                                <div class="skin-minimal mt-40">
                                    <span class="p-dlb w-80"></span>
                                    <a href="javascript:void(0);" class="p-dlb Btn mr-35 accNex">取消</a>
                                    <a href="javascript:void(0);" class="p-dlb Btn getVal">确定</a>
                                </div>
                            </div>
                        </li>
                        <li class="f-14 pb-20 mb-20 p-colorB bt">
                            <div class="viewShow">
                                <span class="p-dlb w-80">性别</span><span class="dot">:</span><span class="result">
                                    @{
                                        var sexString = userInfo.Sex == Bode.Services.Core.Models.User.Sex.男 ? "男" : userInfo.Sex == Bode.Services.Core.Models.User.Sex.女 ? "女" : "保密";}@sexString
                                    </span>
                                    <div class="f-r"><a href="javascript:void(0);" class="p-dlb editor mt-5 mr-10 editN"></a><span class="">编辑</span></div>
                                </div>
                                <div class="viewShow hide">
                                    <span class="p-dlb w-80">性别</span>
                                    <div class="skin-minimal p-dlb">
                                        <div class="radio-box">
                                            <input type="radio" id="radio-1" name="demo-radio1">
                                            <label for="radio-1" data-id="1">男</label>
                                        </div>
                                        <div class="radio-box">
                                            <input type="radio" id="radio-1" name="demo-radio1">
                                            <label for="radio-1" data-id="2">女</label>
                                        </div>
                                        <div class="radio-box">
                                            <input type="radio" id="radio-1" name="demo-radio1">
                                            <label for="radio-1" data-id="0">保密</label>
                                        </div>
                                    </div>
                                    <div class="skin-minimal mt-40">
                                        <span class="p-dlb w-80"></span>
                                        <a href="javascript:void(0);" class="p-dlb Btn mr-35 accNex">取消</a>
                                        <a href="javascript:void(0);" class="p-dlb Btn sex">确定</a>
                                    </div>
                                </div>
                            </li>
                            <li class="f-14 pb-20 mb-20 p-colorB bt">
                                <div class="viewShow">
                                    <span class="p-dlb w-80">生日</span><span class="dot">:</span><span class="result">
                                        @{
                                            var birthday = "";
                                            if (userInfo.Birthday != null)
                                            {
                                                birthday = DateTime.Parse(userInfo.Birthday.ToString()).ToString("yyyy/MM/dd");
                                            }
                                        }
                                        @birthday
                                    </span>
                                    <div class="f-r"><a href="javascript:void(0);" class="p-dlb editor mt-5 mr-10 editN"></a><span class="">编辑</span></div>
                                </div>
                                <div class="viewShow hide">
                                    <span class="p-dlb w-80">生日</span>
                                    <input type="text" id="datetimepicker2" class="" />
                                    <div class="skin-minimal mt-40">
                                        <span class="p-dlb w-80"></span>
                                        <a href="javascript:void(0);" class="p-dlb Btn mr-35 accNex">取消</a>
                                        <a href="javascript:void(0);" class="p-dlb Btn getSelect">确定</a>
                                    </div>
                                </div>
                            </li>
                            <li class="f-14 pb-20 mb-20 p-colorB bt">
                                <div class="viewShow">
                                    <span class="p-dlb w-80">联系电话</span><span class="dot">:</span><span class="result">@userInfo.SysUser.PhoneNumber</span>
                                    <div class="f-r"><a href="javascript:void(0);" class="p-dlb editor mt-5 mr-10 editN"></a><span class="">编辑</span></div>
                                </div>
                                <div class="viewShow hide">
                                    <span class="p-dlb w-80">联系电话</span>
                                    <div class="skin-minimal p-dlb">
                                        <input type="text" placeholder="" class="input-text size-M" id="tele" name="tele" value="@userInfo.SysUser.PhoneNumber">
                                    </div>
                                    <div class="skin-minimal mt-40">
                                        <span class="p-dlb w-80"></span>
                                        <a href="javascript:void(0);" class="p-dlb Btn mr-35 accNex">取消</a>
                                        <a href="javascript:void(0);" class="p-dlb Btn getVal">确定</a>
                                    </div>
                                </div>
                            </li>
                            <li class="f-14 pb-20 mb-20 p-colorB bt hidden">
                                <div class="viewShow">
                                    <span class="p-dlb w-80">邮箱</span><span class="dot">:</span><span class="result">
                                        @userInfo.SysUser.Email
                                    </span>
                                    <div class="f-r"><a href="javascript:void(0);" class="p-dlb editor mt-5 mr-10 editN"></a><span class="">绑定</span></div>
                                </div>
                                <div class="viewShow hide">
                                    <span class="p-dlb w-80">邮箱</span>
                                    <div class="skin-minimal p-dlb">
                                        <input type="email" id="Email" name="email" placeholder="" class="input-text size-M" value="@userInfo.SysUser.Email">
                                    </div>
                                    <div class="skin-minimal mt-40">
                                        <span class="p-dlb w-80"></span>
                                        <a href="javascript:void(0);" class="p-dlb    Btn mr-35 accNex">取消</a>
                                        <a href="javascript:void(0);" class="p-dlb    Btn getVal">确定</a>
                                    </div>
                                </div>
                            </li>
                            <li class="f-14 pb-20 mb-20 p-colorB bt">
                                <div class="viewShow">
                                    <span class="p-dlb w-80">QQ</span><span class="dot">:</span><span class="result">@userInfo.Qq</span>
                                    <div class="f-r"><a href="javascript:void(0);" class="p-dlb editor mt-5 mr-10 editN"></a><span class="">编辑</span></div>
                                </div>
                                <div class="viewShow hide">
                                    <span class="p-dlb w-80">QQ</span>
                                    <div class="skin-minimal p-dlb">
                                        <input type="text" placeholder="" class="input-text size-M" name="QQ" id="QQ" value="@userInfo.Qq">
                                    </div>
                                    <div class="skin-minimal mt-40">
                                        <span class="p-dlb w-80"></span>
                                        <a href="javascript:void(0);" class="p-dlb  Btn mr-35 accNex">取消</a>
                                        <a href="javascript:void(0);" class="p-dlb  Btn getVal">确定</a>
                                    </div>
                                </div>
                            </li>
                        </ul>
                        <div class="add_addr text-c mt-20">
                            <a href="javascript:void();" class=" Btn" onclick="saveInfo()">保存修改</a>
                        </div>
                    </form>
                </div>
                <!--账号安全-->
                <div class="accountCon-bar personalCon-bar">
                    <p class="f-16 mb-40">账号安全</p>
                    <ul>
                        <li class="f-16 pb-20 mb-20 p-colorB bt">
                            <form id="accountOneValid" action="" method="post">
                                <div class="viewShow account-info-0">
                                    <span class="p-dlb w-80">手机号</span><span class="dot">:</span>
                                    <span class="ml-20" id="phoneNo1">
                                        @{
                                            string phoneNo = "";
                                            if (!string.IsNullOrWhiteSpace(userInfo.SysUser.PhoneNumber))
                                            {
                                                phoneNo = userInfo.SysUser.PhoneNumber.Substring(0, 3) + "****" + userInfo.SysUser.PhoneNumber.Substring(userInfo.SysUser.PhoneNumber.Length - 4, 4);
                                            }}@phoneNo
                                        </span>
                                        <div class="f-r"><a href="javascript:void(0);" class="p-dlb editor mt-5 mr-10 editN"></a><span>编辑</span></div>
                                    </div>
                                    <div class="viewShow account-info-1 hide">
                                        <span class="p-dlb w-80">手机号</span>
                                        <div class="skin-minimal p-dlb">
                                            <span class="ml-14">已绑定手机号：</span>
                                            <span id="phoneNo2">@phoneNo</span>
                                        </div>
                                        <div class="skin-minimal m28 pos-r">
                                            <span class="p-dlb w-80"></span>
                                            <input type="password" placeholder="输入登录密码" class="input-text radius size-M input" id="cvOnePass" name="cvOnePass">
                                        </div>
                                        <div class="skin-minimal mt-40">
                                            <span class="p-dlb w-80"></span>
                                            <a href="javascript:void(0);" class="p-dlb    Btn mr-35 accRet">取消</a>
                                            <a href="javascript:void(0);" class="p-dlb    Btn getAccoVal" data-pwd="">下一步</a>
                                        </div>
                                    </div>
                                    <div class="viewShow account-info-2 hide">
                                        <span class="p-dlb w-80">手机号</span>
                                        <input type="text" placeholder="输入新手机号" class="input-text radius size-M input p-dlb" id="cvOneTele1" name="cvOneTele1" data-name="@userInfo.SysUser.UserName" />
                                        <div class="captcha mt-20">
                                            <span class="p-dlb w-80"></span>
                                            <input type="text" placeholder="输入短信验证码" class="input-text radius size-M input" id="cvOneTele2" name="cvOneTele2" />
                                            <a href="javascript:void(0);" class="p-dlb" onclick="sendValidateCode(this,3);">获取验证码</a>
                                        </div>
                                        <div class="skin-minimal mt-40">
                                            <span class="p-dlb w-80"></span>
                                            <a href="javascript:void(0);" class="p-dlb    Btn mr-40 accRet">取消</a>
                                            <a href="javascript:void(0);" class="p-dlb    Btn getAccoVal" data-va="1">下一步</a>
                                        </div>
                                    </div>
                                    <div class="viewShow account-info-3 hide">
                                        <span class="p-dlb w-80">手机号</span>
                                        <h4>您的手机号已修改成功！</h4>
                                        <div class="skin-minimal">
                                            <span class="p-dlb w-80"></span>
                                            <a href="javascript:void(0);" class="p-dlb    Btn mr-35 invisible accRet">取消</a>
                                            <a href="javascript:void(0);" class="p-dlb    Btn accNex">完成</a>
                                        </div>
                                    </div>
                                </form>
                            </li>
                            <li class="f-16 pb-20 mb-20 p-colorB bt">
                                <form id="accountTwoValid" action="" method="post">
                                    <div class="viewShow account-info-0">
                                        <span class="p-dlb w-80">密码</span><span class="dot">:</span>
                                        <span class="ml-20">*******</span>
                                        <div class="f-r"><a href="javascript:void(0);" class="p-dlb editor mt-5 mr-10  editN"></a><span>编辑</span></div>
                                    </div>
                                    <div class="viewShow account-info-1 hide">
                                        <span class="p-dlb w-80">登录密码</span>
                                        <div class="skin-minimal p-dlb">
                                            <span class="ml-14">已绑定手机号：</span>
                                            <span>@phoneNo</span>
                                        </div>
                                        <div class="captcha m28">
                                            <span class="p-dlb w-80"></span>
                                            <input type="text" placeholder="输入验证码" class="input-text radius size-M input" name="cvTwoTele" id="cvTwoTele">
                                            <a href="javascript:void(0);" class="p-dlb" onclick="sendValidateCode(this,2);">获取验证码</a>
                                        </div>
                                        <div class="skin-minimal mt-40">
                                            <span class="p-dlb w-80"></span>
                                            <a href="javascript:void(0);" class="p-dlb    Btn mr-35 accRet">取消</a>
                                            <a href="javascript:void(0);" class="p-dlb    Btn getAccoVal">下一步</a>
                                        </div>
                                    </div>
                                    <div class="viewShow account-info-2 hide">
                                        <span class="p-dlb w-80">新的密码</span>
                                        <div class="skin-minimal p-dlb">
                                            <input type="password" placeholder="输入新密码" class="input-text radius size-M input" id="cvTwoPass" name="cvTwoPass">
                                        </div>
                                        <div class="skin-minimal mt-40">
                                            <span class="p-dlb w-80"></span>
                                            <a href="javascript:void(0);" class="p-dlb    Btn mr-40 accRet">取消</a>
                                            <a href="javascript:void(0);" class="p-dlb    Btn getAccoVal" data-va="2">下一步</a>
