            <script type="text/javascript">
                //导航栏active变更
                $("#topMulu").find('li').find('a').removeClass('active');
                $("#LoginState").attr('href', "");
                $(function () {
                    getOrderData(1);
                    getAfterData(1);
                    $("#orderUl li a").on("click", function () {
                        //$(this).parents(".personalCon-bar").find(".disAll").css({
                        //    "fontWeight": "500",
                        //    "color": "#999"
                        //});;
                        $("#orderUl").find('.current').removeClass('current');
                        $(this).parent().addClass('current');
                        getOrderData(1);
                    });
                    $("#shouhouUl li a").on("click", function () {
                        //$(this).parents(".personalCon-bar").find(".disAll").css({
                        //    "fontWeight": "500",
                        //    "color": "#999"
                        //});;
                        $("#shouhouUl").find('.current').removeClass('current');
                        $(this).parent().addClass('current');
                        getAfterData(1);
                    });

                });

                function getOrderData(pageIndex) {
                    var state = $('#orderUl').find('.current').find('a').attr('data-id');
                    $.post("/Web/Account/GetOrderStateAll", { pageIndex: pageIndex, pageSize: 5, state: state }, function (data) {
                        if (data === "0") {
                            window.location.href = "/Web/Home/Login";
                            return;
                        }
                        $("#bar" + state + "").html(data);
                    });
                }

                function getAfterData(pageIndex) {
                    var state = $('#shouhouUl').find('.current').find('a').attr('data-id');
                    $.post("/Web/Account/GetAfterData", { pageIndex: pageIndex, pageSize: 5, state: state }, function (data) {
                        if (data === "0") {
                            window.location.href = "/Web/Home/Login";
                            return;
                        }
                        $("#s_bar" + state + "").html(data);
                    });
                }
            </script>

