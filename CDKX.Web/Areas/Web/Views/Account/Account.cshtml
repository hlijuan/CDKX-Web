@{
    Layout = null;
    var userInfo = (Bode.Services.Core.Models.User.UserInfo)ViewBag.UserInfo;
    List<Bode.Services.Core.Models.CustomUser.Position> positions = ViewBag.Positions;

}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>个人中心</title>
    <!-- 屏幕适配 -->
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
