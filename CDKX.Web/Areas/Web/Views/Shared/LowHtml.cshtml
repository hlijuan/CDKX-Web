                        <h3 class="f-16">帮助中心</h3>
                        <p class="f-14">您的购物指南</p>
                    </div>
                </li>
            </ul>
        </div>
    </section>
    <section class="foot-other">
        <div class="other-con con">
            <ul class="items clearfix text-c">
                @{
                    var id = 1;
                    foreach (Bode.Services.Core.Models.Help.Muemu mu in ViewBag.Muemu)
                    {
                        <li class="item col-4-1 l">
                            <h3 class="f-16">@mu.Name</h3>
                            <ul>
                                @{var mu1 = mu;
                                foreach (Bode.Services.Core.Models.Help.Content content in contentList.Where(x => x.Muemu.Id == mu1.Id))
                                {
                                    <li><a href="/Web/Home/Help?id=@content.Id">@content.Title</a></li>
                                    id++;
                                }
                                }
                            </ul>
                        </li>
                    }
                }
